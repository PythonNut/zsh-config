#compdef clonedigger
#
# this is zsh completion function file.
# generated by genzshcomp(ver: 0.2.2)
#

typeset -A opt_args
local context state line

_arguments -s -S \
  "--help[show this help message and exit]:" \
  "-h[show this help message and exit]:" \
  "--language[the programming language]::LANGUAGE:_files" \
  "-l[the programming language]::LANGUAGE:_files" \
  "--no-recursion[do not traverse directions recursively]" \
  "--output[the name of the output file (\"output.html\" by default)]::OUTPUT:_files" \
  "-o[the name of the output file (\"output.html\" by default)]::OUTPUT:_files" \
  "--clustering-threshold[read the paper for semantics]::CLUSTERING_THRESHOLD:_files" \
  "--distance-threshold[the maximum amount of differences between pair of sequences in clone pair (5 by default). Larger value leads to larger amount of false positives]::DISTANCE_THRESHOLD:_files" \
  "--hashing-depth[default value if 1, read the paper for semantics. Computation can be speeded up by increasing this value (but some clones can be missed)]::HASHING_DEPTH:_files" \
  "--size-threshold[the minimum clone size. The clone size for its turn is equal to the count of lines of code in its the largest fragment]::SIZE_THRESHOLD:_files" \
  "--clusterize-using-dcup[mark each statement with its D-cup value instead of the most similar pattern. This option together with --hashing-depth=0 make it possible to catch all considered clones (but it is slow and applicable only to small programs)]" \
  "--dont-print-time[do not print time]" \
  "--force" \
  "-f" \
  "--force-diff[force highlighting of differences based on the diff algorithm]" \
  "--fast[find only clones, which differ in variable and function names and constants]" \
  "--ignore-dir[exclude directories from parsing]::IGNORE_DIRS:_files" \
  "--eclipse-output[for internal usage only]::ECLIPSE_OUTPUT:_files" \
  "--cpd-output[output as PMDs CPDs XML format. If output file not defined, output.xml is generated]" \
  "--report-unifiers" \
  "--func-prefixes[skip functions/methods with these prefixes (provide a CSV string as argument)]::F_PREFIXES:_files" \
  "--file-list[a file that contains a list of file names that must be processed by Clone Digger]::FILE_LIST:_files" \
  "*::args:_files"
